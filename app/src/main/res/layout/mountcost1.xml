<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/gardencardview1"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">


            <TextView
                android:id="@+id/maoundd"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_marginStart="50dp"
                android:layout_marginTop="50dp"
                android:text="AY:"
                android:textSize="20sp"
                android:textStyle="bold"/>

            <TextView
                android:id="@+id/mountinfo"
                android:layout_width="38dp"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="90dp"
                android:layout_marginTop="50dp"
                android:text="ay"
                android:textSize="20sp"
                app:layout_constraintStart_toEndOf="@id/maoundd"
                app:layout_constraintTop_toTopOf="parent" />
            <TextView
                android:id="@+id/yeardee"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_marginStart="150dp"
                android:layout_marginTop="50dp"
                android:text="YIL:"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintStart_toEndOf="@id/mountinfo"/>
            <TextView
                android:id="@+id/yearinfo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="200dp"
                android:layout_marginTop="50dp"
                android:text="yıl"
                android:textSize="20sp"
                app:layout_constraintStart_toEndOf="@id/yeardee"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tarlaAdıLabel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_marginStart="20dp"
                android:layout_marginTop="100dp"
                android:text="Yakıt:"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintStart_toEndOf="@id/yearinfo"/>


            <TextView
                android:id="@+id/yakıtinfo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="180dp"
                android:layout_marginTop="99dp"
                android:text="yakıt"
                android:textSize="20sp"
                app:layout_constraintStart_toEndOf="@id/tarlaAdıLabel"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/area"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/tarlaAdıLabel"
                android:layout_alignParentStart="true"
                android:layout_marginStart="20dp"
                android:layout_marginTop="7dp"
                android:text="Tohum:"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tohuminfo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="180dp"
                android:layout_marginTop="135dp"
                android:text="tohum"
                android:textSize="20sp"
                app:layout_constraintStart_toEndOf="@id/area"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/adanumber"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/area"
                android:layout_alignParentStart="true"
                android:layout_marginStart="20dp"
                android:layout_marginTop="13dp"
                android:text="Sulama:"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/sulamainfo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="180dp"
                android:layout_marginTop="171dp"
                android:text="sulama"
                android:textSize="20sp"
                app:layout_constraintStart_toEndOf="@id/adanumber"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/parcel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/adanumber"
                android:layout_alignParentStart="true"
                android:layout_marginStart="20dp"
                android:layout_marginTop="13dp"
                android:text="İş gücü:"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/isgücüinfo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="180dp"
                android:layout_marginTop="210dp"
                android:text="is gücü"
                android:textSize="20sp"
                app:layout_constraintStart_toEndOf="@id/parcel"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/city"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/parcel"
                android:layout_alignParentStart="true"
                android:layout_marginStart="20dp"
                android:layout_marginTop="13dp"
                android:text="Makına:"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/makınainfo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="180dp"
                android:layout_marginTop="257dp"
                android:text="makına"
                android:textSize="20sp"
                app:layout_constraintStart_toEndOf="@id/city"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/town"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/city"
                android:layout_alignParentStart="true"
                android:layout_marginStart="20dp"
                android:layout_marginTop="13dp"
                android:text="İlaç:"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/ilacinfo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="180dp"
                android:layout_marginTop="295dp"
                android:text="İlaç"
                android:textSize="20sp"
                app:layout_constraintStart_toEndOf="@id/town"
                app:layout_constraintTop_toTopOf="parent" />


            <TextView
                android:id="@+id/district"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/town"
                android:layout_alignParentStart="true"
                android:layout_marginStart="20dp"
                android:layout_marginTop="13dp"
                android:text="Organik gübre:"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/organikgübreinfo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="180dp"
                android:layout_marginTop="337dp"
                android:text="Organik gübre"
                android:textSize="20sp"
                app:layout_constraintStart_toEndOf="@id/district"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/deneme"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/district"
                android:layout_alignParentStart="true"
                android:layout_marginStart="20dp"
                android:layout_marginTop="13dp"
                android:text="Kimyasal Gübre:"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/kimyasalgübreinfo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="180dp"
                android:layout_marginTop="375dp"
                android:text="Kimyasal Gübre"
                android:textSize="20sp"
                app:layout_constraintStart_toEndOf="@id/deneme"
                app:layout_constraintTop_toTopOf="parent" />


        </RelativeLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.cardview.widget.CardView>